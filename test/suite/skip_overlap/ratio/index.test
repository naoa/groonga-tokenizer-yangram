register tokenizers/yangram

config_set tokenizer.yangram.Terms.skip_overlap_ratio 0.5

table_create Terms TABLE_HASH_KEY ShortText \
  --default_tokenizer TokenYaBigram \
  --normalizer NormalizerAuto

table_create Memos TABLE_NO_KEY
column_create Memos title COLUMN_SCALAR ShortText
column_create Memos content COLUMN_SCALAR ShortText

load --table Memos
[
{"title": "今日", "content": "今日は雨だな"},
{"title": "今日", "content": "明日が雨だな"},
{"title": "今日", "content": "明日が雨だな"},
{"title": "今日", "content": "明日が雨だな"},
{"title": "今日", "content": "明日が雨だな"},
{"title": "今日", "content": "明日が雨だな"}
]

column_create Terms memos_content COLUMN_INDEX|WITH_POSITION Memos content
column_create Terms @memos_title COLUMN_INDEX|WITH_POSITION Memos title

table_tokenize Terms "今日は雨だな" --mode GET
